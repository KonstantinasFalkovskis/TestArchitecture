<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Regression tests suite" verbose = "1">
<parameter name="environment" value ="local"></parameter>
  <test name="MainPage regression tests in chrome browser">
  <parameter name="browserName" value ="chrome"/>
  <parameter name="url" value ="https://www.airbnb.com/?locale=en"></parameter>
  <groups>
	<define name="all">
		<include name="mainPageRegression"></include>
	</define>
	<run>
		<include name="all"></include>
	</run>
	</groups>
	    <classes>
	      <class name="tests.MainPageTest"/>
	    </classes>
    </test>
</suite> 
